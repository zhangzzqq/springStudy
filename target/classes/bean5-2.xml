<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="tyre1Bean" class="com.zq.study.framework5.two.Tyres">
        <property name="name" value="MRF">
<!--        <property name="name" value="TVS">-->
        </property>

        <property name="place" value="India">
        </property>

        <property name="message" value="Make in India">
        </property>
    </bean>

    <bean id="ToyotaBean" class="com.zq.study.framework5.two.ToyotaEngine">
        <property name="company" value="Toyota">
        </property>

        <property name="cost" value="300000.00">
        </property>

    </bean>


    <bean id="tyre2Bean" class="com.zq.study.framework5.two.Tyres">
        <property name="name" value="TVS">
        </property>

        <property name="place" value="India">
        </property>

        <property name="message" value="Make in India">
        </property>
    </bean>


    <bean id="InjectwithSetter" class="com.zq.study.framework5.two.Vehicle">
        <property name="engine" ref="ToyotaBean">
        </property>

        <property name="tyre" ref="tyre1Bean">
        </property>
    </bean>


    <bean id="InjectwithConstructor" class="com.zq.study.framework5.two.Vehicle">
        <constructor-arg name="engine" ref="ToyotaBean">
        </constructor-arg>

        <constructor-arg name="tyre" ref="tyre2Bean">
        </constructor-arg>

    </bean>


</beans>
